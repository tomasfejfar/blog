---
sitemap: false
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom" xml:lang="en">
<title type="text">{{ title }}</title>
<generator uri="https://github.com/jekyll/jekyll">Jekyll</generator>
<link rel="self" type="application/atom+xml" href="{{ public_url }}/feed.xml" />
<link rel="alternate" type="text/html" href="{{ public_url }}" />
<updated>{{ time | date_to_xmlschema }}</updated>
<id>{{ public_url }}/</id>
<author>
  <name>{{ owner.name }}</name>
  <uri>{{ public_url }}/</uri>
  {% if owner.email %}<email>{{ owner.email }}</email>{% endif %}
</author>
{% for post in posts limit:20 %}
{% if post.author %}
  {% assign author = data.authors[post.author] %}
{% else %}
  {% assign author = owner %}
{% endif %}
<entry>
  <title type="html"><![CDATA[{{ post.title | cdata_escape }}]]></title>
  <link rel="alternate" type="text/html" href="{{ public_url }}{{ post.url }}" />
  <id>{{ public_url }}{{ post.id }}</id>
  {% if post.modified %}<updated>{{ post.modified | to_xmlschema }}T00:00:00-00:00</updated>
  <published>{{ post.date | date_to_xmlschema }}</published>
  {% else %}<published>{{ post.date | date_to_xmlschema }}</published>
  <updated>{{ post.date | date_to_xmlschema }}</updated>{% endif %}
  <author>
    <name>{{ author.name }}</name>
    <uri>{{ public_url }}</uri>
    {% if author.email %}<email>{{ author.email }}</email>{% endif %}
  </author>
  <content type="html">
    {{ post.content | xml_escape }}
    {% include _snippets/_feed-footer.twig %}
  </content>
</entry>
{% endfor %}
</feed>
